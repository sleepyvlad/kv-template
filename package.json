{
  "name": "kv-template",
  "version": "1.0.0",
  "description": "template for projects",
  "main": "index.js",
  "scripts": {
    "dev": "webpack --mode development",
    "start": "tcm -p src/**/*.pcss && webpack serve --open --config webpack-configs/webpack.dev.js",
    "build": "webpack --config webpack-configs/webpack.prod.js",
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "generate": "plop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sleepyvlad/kv-template.git"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "template"
  ],
  "author": "Karimov Vladislav",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sleepyvlad/kv-template/issues"
  },
  "homepage": "https://github.com/sleepyvlad/kv-template#readme",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "babel": "^6.23.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.18.0",
    "eslint-plugin-react": "^7.22.0",
    "handlebars": "^4.7.6",
    "html-webpack-plugin": "^4.5.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "plop": "^2.7.4",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.2.0",
    "postcss-modules": "^4.0.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.14",
    "typed-css-modules": "^0.6.4",
    "typescript": "^4.1.3",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
